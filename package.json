{
  "name": "@nordicsemiconductor/thingy-rocks-cloud",
  "version": "0.0.0-development",
  "description": "Cloud backend for the thingy.rocks AWS IoT dashboard developed using AWS CDK in TypeScript.",
  "type": "module",
  "scripts": {
    "test": "npx tsx --test ./**/*.spec.ts",
    "prepare": "husky && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicPlayground/thingy-rocks-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicPlayground/thingy-rocks-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicPlayground/thingy-rocks-cloud-aws-js#readme",
  "keywords": [
    "thingy-world",
    "iot",
    "aws",
    "cdk",
    "demo"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "3.515.0",
    "@aws-sdk/client-dynamodb": "3.518.0",
    "@aws-sdk/client-iot": "3.515.0",
    "@aws-sdk/client-iot-data-plane": "3.515.0",
    "@aws-sdk/client-ssm": "3.519.0",
    "@aws-sdk/client-timestream-query": "3.515.0",
    "@aws-sdk/util-dynamodb": "3.518.0",
    "@bifravst/eslint-config-typescript": "6.0.5",
    "@bifravst/prettier-config": "1.0.0",
    "@commitlint/config-conventional": "18.6.2",
    "@protobuf-ts/plugin": "2.9.3",
    "@swc/cli": "0.3.9",
    "@swc/core": "1.4.2",
    "@types/aws-lambda": "8.10.134",
    "@types/glob": "8.1.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.20",
    "@types/yazl": "2.4.5",
    "aws-cdk": "2.130.0",
    "aws-cdk-lib": "2.130.0",
    "cdk": "2.130.0",
    "chalk": "5.3.0",
    "check-node-version": "4.2.1",
    "constructs": "10.3.0",
    "glob": "10.3.10",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "tsx": "4.7.1",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "dependencies": {
    "@aws-sdk/client-s3": "3.515.0",
    "@hello.nrfcloud.com/proto-lwm2m": "2.4.6",
    "@nordicsemiconductor/from-env": "3.0.1",
    "@nordicsemiconductor/timestream-helpers": "6.0.2",
    "@protobuf-ts/runtime": "2.9.3",
    "@sinclair/typebox": "0.32.14",
    "ajv": "8.12.0",
    "aws-iot-device-sdk-v2": "1.19.1",
    "jsonata": "2.0.3",
    "lodash-es": "4.17.21",
    "mqtt": "5.3.5",
    "p-throttle": "6.1.0",
    "protobufjs": "7.2.6"
  }
