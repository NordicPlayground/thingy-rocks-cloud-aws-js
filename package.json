{
  "name": "@nordicsemiconductor/thingy-rocks-cloud",
  "version": "0.0.0-development",
  "description": "Cloud backend for the thingy.rocks AWS IoT dashboard developed using AWS CDK in TypeScript.",
  "type": "module",
  "scripts": {
    "test": "jest",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicPlayground/thingy-rocks-cloud-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicPlayground/thingy-rocks-cloud-aws-js/issues"
  },
  "homepage": "https://github.com/NordicPlayground/thingy-rocks-cloud-aws-js#readme",
  "keywords": [
    "thingy-world",
    "iot",
    "aws",
    "cdk",
    "demo"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "3.236.0",
    "@aws-sdk/client-dynamodb": "3.236.0",
    "@aws-sdk/client-iot": "3.236.0",
    "@aws-sdk/client-iot-data-plane": "3.236.0",
    "@aws-sdk/client-timestream-query": "3.236.0",
    "@aws-sdk/util-dynamodb": "3.236.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.160",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "10.0.58",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.24",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "8.10.109",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.17",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.56.0",
    "cdk": "2.56.0",
    "chalk": "5.2.0",
    "check-node-version": "4.2.1",
    "constructs": "10.1.197",
    "glob": "8.0.3",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "tessera": "0.15.3",
    "tilelive-aws": "3.2.0",
    "tsx": "3.12.1",
    "typescript": "4.9.4",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@nordicsemiconductor/from-env": "1.0.3",
    "@nordicsemiconductor/timestream-helpers": "4.1.3",
    "@sinclair/typebox": "0.25.16",
    "ajv": "8.11.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
